<div class="columns-12">
  <h4 style="text-align: center">
    Find out admin's password.<br>
    That's it.<br>
    Here:<%= link_to 'there', @url %>
  </h4>
  <hr>
  <div class="col-6 col-offset-3">
    <%= form_tag "/wargame/web/level1" do %>
        <%= label_tag :password %>
        <%= password_field_tag :password %>
        <%= submit_tag 'Submit', class: 'btn btn-success' %>
    <% end %>
  </div>
</div>
<%= render 'shared/each_rank', ranked_players: @ranked_players, index: 5 %>