  <code>
    import hashlib</br>
    </br>
    def mix_code(msg, salt):</br>
    &nbsp;&nbsp;&nbsp;&nbsp;token = hashlib.md5(salt).digest()</br>
    &nbsp;&nbsp;&nbsp;&nbsp;res = ""</br>
    &nbsp;&nbsp;&nbsp;&nbsp;password = "NSYSU-WARGAME-CRYPTO"</br>
    &nbsp;&nbsp;&nbsp;&nbsp;for c in msg:</br>
